    | IDENT:i OPRESTA prAll {:auxSet=i;:} prAll LeerParaBorrarElem_set prAll
    | IDENT:i OPINTER prAll {:auxSet=i;Generator.asig_Array_ALL=true;:} prAll LeerParaInter_set:t prAll{:TablaSimbolos.VolcarArrayAux_a_Original_SET(i,t,true);:}
   
    | SET Ids declarar_set
 
